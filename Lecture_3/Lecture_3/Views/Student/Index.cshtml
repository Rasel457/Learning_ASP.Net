@model List<Lecture_3.Models.Entity.Student>
@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>
<a href="/Student/Create" class="btn btn-primary">Add Student</a>
<br />
<table class="table table-strriped">
    <tr>
        <th>Id</th>
        <th>Name</th>
        <th>Dob</th>
        <th>Gender</th>
        <th>Cgpa</th>
    </tr>
    @foreach (var item in Model)
    {
        <tr>
            <td>@item.Id</td>
            <td>@item.Name</td>
            <td>@item.Dob</td>
            <td>@item.Gender</td>
            <td>@item.Cgpa</td>
        </tr>
    }
</table>
