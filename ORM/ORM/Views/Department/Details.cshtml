@model ORM.Models.Department
@{
    ViewBag.Title = "Details";
}
<h2>Details</h2>
<h1>Name : @Model.Name</h1>
<h3>List of Students</h3>
<table class="table table-bordered">
    <tr>
        <th>Id</th>
        <th>Name</th>
        <th>Dob</th>
        <th>Gender</th>
        <td>Cgpa</td>
    </tr>
    @foreach (var s in Model.Students)
    {
        <tr>
            <td>@s.Id</td>
            <td>@s.Name</td>
            <td>@s.Dob</td>
            <td>@s.Gender</td>
            <td>@s.Cgpa</td>
        </tr>

    }
</table>


