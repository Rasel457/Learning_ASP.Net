@model List<Lecture_5.Models.Entity.Product>
@{
    ViewBag.Title = "Checkout";
}

<h2>Checkout</h2>

<table class="table table-hover">
    <tr>
        <th>Id</th>
        <th>Name</th>
        <th>Price</th>
        <th>Quantity</th>
        <th>Description</th>
    </tr>


    @foreach (var item in Model)
    {
        if (item != null)
        {
            <tr>
                <td>@item.Id</td>
                <td>@item.Name</td>
                <td>@item.Price</td>
                <td>@item.Quantity</td>
                <td>@item.Description</td>
            </tr>

        }

    }

</table>


